{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "framework": "vite",
    "buildCommand": "yarn install --frozen-lockfile && yarn build",
    "devCommand": "yarn dev",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  